    }
  ],
  "dependencies": {
    "azure-common": "0.9.0-pre.1",
    "azure-mgmt-compute": "0.9.0-pre.1",
    "azure-mgmt": "0.9.0-pre.1",
    "azure-mgmt-vnet": "0.9.0-pre.1",
    "azure-mgmt-sb": "0.9.0-pre.1",
    "azure-mgmt-sql": "0.9.0-pre.1",
    "azure-mgmt-storage": "0.9.0-pre.1",
    "azure-mgmt-store": "0.9.0-pre.1",
    "azure-mgmt-subscription": "0.9.0-pre.1",
    "azure-mgmt-website": "0.9.0-pre.1",
    "mime": ">= 1.2.4",
    "underscore": ">= 1.3.1",
    "request": "2.27.0",
